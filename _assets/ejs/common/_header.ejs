<% let title = (pageId === 'home') ? jsonData.siteInfo.name : jsonData.pages[pageId].name + ' | '  + jsonData.siteInfo.name%>
<% let description = jsonData.pages[pageId].description %>
<!DOCTYPE html>
<html lang="ja">

<head>
  <meta charset="utf-8">
  <meta name="format-detection" content="telephone=no">
  <meta name="viewport" content="width=device-width">
  <link rel="apple-touch-icon" href="/img/common/apple-touch-icon.png">

  <title><%= title %></title>
  <meta name="description" content="<%= description %>">

  <meta property="og:title" content="<%= title %>">
  <meta property="og:description" content="<%= description %>">
  <meta property="og:url" content="<%= jsonData.siteInfo.siteUrl + jsonData.pages[pageId].slug %>">
  <meta property="og:type" content="<%= jsonData.pages[pageId].type %> ">
  <meta property="og:local" content="ja_JP">
  <meta property="og:image" content="/common/og-image.jpg">
  <meta property="og:site_name" content="<%= jsonData.siteInfo.siteUrl %> ">

  <link rel="stylesheet" type="text/css" href="/css/style.css">

  <script src="/js/lib/b_browser_switcher.js"></script>
</head>

<body>

  <div id="wrapper">
    <header id="header" class="p-header">
      <div class="p-header__inner">
        <a href="./" class="p-header__logo">
          <h1>
            <img src="../../img/common/header-logo.svg" alt="いいサイクル千代田">
          </h1>
        </a>
        <nav class="p-header__nav">
          <ul class="p-header__menu">
            <% for(let page in jsonData.pages){ %>
            <% if (page === pageId) { %>
            <!-- <li class="current"> -->
                <!-- <%= jsonData.pages[page].name %> -->
            <% } else { %>
              <% console.log(page) %>              <% if (page === 'about') { %>
                <li>
                  <a href="<%= jsonData.pages[page].slug %>"><img src="../img/svg/logo.svg" alt="e.cycle">とは
                  </a>
                </li>
                      <% } else { %>
                
              <li>
                <a href="<%= jsonData.pages[page].slug %>"><%- jsonData.pages[page].name %>
                  <% if (typeof jsonData.pages[page].subpages !== 'undefined') { %>
                    <span></span>
                    <% }; %>
                  </a>
                <% if (typeof jsonData.pages[page].subpages !== 'undefined') { %>
                  <ul class="p-header__submenu">
                  <% for(let subpage in jsonData.pages[page].subpages){ %>
                    <% console.log(jsonData.pages[page].subpages[subpage].name); %>
                    <li>
                      <a href="<%= jsonData.pages[page].subpages[subpage].slug %>"><%- jsonData.pages[page].subpages[subpage].name %>
                      </a>
                    </li>
                    <% }; %>
                  </li>
                </ul>
              <% }; %>
            <% }; %>
            </li>
            <% } %>
            <% }; %>
          </ul>
        </nav>
      </div><!-- /l-inner -->
    </header>
